<%- include("header") -%>
<body>
    <div class="box" id="heading">
        <h1><%= listHeading %></h1>
    </div>
    <h2></h2>
    <ul>
        <div class="box">
            <% for (var i = 0; i < newItem.length; i++) { %>
                <% if (newItem[i] != "") { %>
                    <div class="item">
                        <input type="checkbox">
                        <p class="itemText"><%= newItem[i].name %></p>
                    </div>
                <% } %>
                <% } %>
                <form class="item" action="/" method="post">
                    <input type="text" name="newItem" placeholder="New Item" autocomplete="off">
                    <button>+</button>
                </form>
        </div>
    </ul>
<%- include("footer") -%>